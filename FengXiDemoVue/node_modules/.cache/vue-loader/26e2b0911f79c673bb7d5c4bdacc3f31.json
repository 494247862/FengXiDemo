rop=\"menuUrl\">\n        <el-input v-model=\"formData.menuUrl\"/>\n      </el-form-item>\n\n      <el-form-item label=\"菜单图标:\"\n                    prop=\"icon\">\n        <el-input v-model=\"formData.icon\"/>\n      </el-form-item>\n\n      <el-form-item label=\"序号:\"\n                    prop=\"number\">\n        <el-input-number v-model=\"formData.number\"/>\n      </el-form-item>\n\n      <el-form-item style=\"text-align: end\">\n        <el-button type=\"primary\" :loading=\"loading\" @click=\"updateData\">保存</el-button>\n        <el-button @click=\"closeFun\">取消</el-button>\n      </el-form-item>\n    </el-form>\n  </div>\n</template>\n\n<script>\n  import {updateMenu} from '@/api/menu'\n\n  export default {\n    name: 'Update',\n    data() {\n      return {\n        loading: false,\n        formData: {}\n      }\n    },\n    props: {\n      row: {\n        type: Object,\n        default: {}\n      },\n      closeFun: {\n        type: Function\n      }\n    },\n    mounted() {\n      this.formData = {...this.row}\n    },\n    methods: {\n      updateData() {\n        this.$refs['updateFormRef'].validate((valid) => {\n          if (valid) {\n            this.loading = true\n            updateMenu(this.formData).then(res => {\n              this.$notify.success({\n                title: '成功',\n                message: '更新成功'\n              })\n              this.loading = false\n              this.closeFun()\n            }).catch(err => {\n              this.$notify.error({\n                title: '失败',\n                message: err.message\n              })\n              this.loading = false\n            })\n          } else {\n            return false\n          }\n        })\n      }\n    }\n  }\n</script>\n\n<style scoped>\n\n</style>\n"]}]}